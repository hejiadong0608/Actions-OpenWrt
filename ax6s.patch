@@ -285,20 +285,7 @@
 		partition@2c0000 {
 			label = "kernel";
 			compatible = "denx,fit";
-			reg = <0x2c0000 0x400000>;
-		};
-
-
-		/* ubi partition is the result of squashing
-		 * consecutive stock partitions:
-		 * - firmware (partially)
-		 * - firmware1
-		 * - overlay
-		 * - obr
-		 */
-		partition@6c0000 {
-			label = "ubi";
-			reg = <0x6C0000 0x6f00000>;
+			reg = <0x2c0000 0x7300000>;
 		};
 	};
 };
